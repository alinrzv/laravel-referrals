{
    "name": "alinrzv/laravel-referrals",
    "description": "A referrals system for a laravel projects.",
    "homepage": "http://github.com/alinrzv/laravel-referrals",
    "type": "library",
    "version": "1.0",
    "require": {
        "php": "^7.0",
        "laravel/framework": "^8.0",
        "ramsey/uuid": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Best Digital",
            "email": "hidev@bestdigital.ro"
        }
